{
  "name": "scrollable",
  "version": "0.0.0",
  "description": "Scrollable components for advanced mobile scrolling with React.js",
  "main": "src/scrollable.js",
  "dependencies": {
    "react": "^0.13.0-rc1",
    "react-tools": "^0.13.0-rc1"
  },
  "devDependencies": {
    "beefy": "^2.1.3",
    "browserify": "^9.0.3",
    "browserify-istanbul": "^0.1.5",
    "istanbul": "^0.3.6",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-browserify": "^3.0.2",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs2-launcher": "^0.1.5",
    "karma-spec-reporter": "0.0.16",
    "react-hammerjs": "^0.1.3",
    "reactify": "^1.0.0",
    "watchify": "^2.3.0"
  },
  "scripts": {
    "build": "browserify examples/main.js -o examples/build/bundle.js      --transform reactify --insert-global-vars React",
    "dev": "beefy examples/main.js:examples/build/bundle.js 8080 --live -- --transform reactify --insert-global-vars React --debug --verbose",
    "test": "karma start karma.headless.js && istanbul report && echo 'Details at ./coverage/index.html'"
  },
  "author": "Irae Carvalho <irae@irae.pro.br>",
  "license": "MIT"
}
